#include <stdio.h>

int main()
{
   int a,b,c;
   printf("enter a,b,c");
   scanf("%d%d%d",&a,&b,&c);

   if(a<b)
   {
       printf("a is smaller");
   }
else if(a<b)
{
    printf("b is smaller");
}
else if(b<c)
{
    printf("b is smaller");
}
else if(b<c)
{
    printf("c is smaller");
}
else if(c<a)
{
    printf("c is smaller");
}
else
{
    printf("a is smaller");
}
    return 0;
}
