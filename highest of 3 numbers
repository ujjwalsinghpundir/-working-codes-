#include <stdio.h>

int main()
{
   int a,b,c;
   printf("enter a,b,c");
   scanf("%d%d%d",&a,&b,&c);
   
   if(a>b)
   {
       printf("a is greater");
   }
else if(a>b)
{
    printf("b is greater");
}
else if(b>c)
{
    printf("b is greater");
}
else if(b>c)
{
    printf("c is greater");
}
else if(c>a)
{
    printf("c is greater");
}
else
{
    printf("a is greater");
}
    return 0;
}
